    "mongodb": "^3.6.2",
    "multer": "^1.4.2",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.28",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/mongodb": "^3.5.27",
    "@types/multer": "^1.4.4",
    "@types/yup": "^0.29.8",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
